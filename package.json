    "web-push": "^3.5.0"
  },
  "devDependencies": {
    "@ucd-lib/cork-app-build": "^0.4.1",
    "nodemon": "^2.0.19",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
